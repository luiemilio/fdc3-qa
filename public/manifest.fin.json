{
	"runtime": {
		"arguments": "--v=1 --inspect",
		"version": "canary"
	},
	"platform": {
		"uuid": "platform-1",
		"icon": "https://www.learningcontainer.com/wp-content/uploads/2020/09/Sample-ICO-Image-for-Testing.ico",
		"autoShow": true,
		"providerUrl": "http://localhost:5050/html/provider.html",
		"apiDiagnostics": false,
		"defaultWindowOptions": {
			"url": "http://localhost:5050/html/window.html",
			"contextMenu": true,
			"defaultWidth": 600,
			"defaultHeight": 600,
			"defaultLeft": 0,
			"defaultTop": 0,
